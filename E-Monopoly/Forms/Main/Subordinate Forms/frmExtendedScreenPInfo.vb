    Private Sub EventDisplayer()
        My.Application.Log.DefaultFileLogWriter.Close()
        rtcHistory.Text = My.Computer.FileSystem.ReadAllText(My.Settings.HistoryFileName)
        rtcHistory.SelectionStart = rtcHistory.Text.Length
        rtcHistory.ScrollToCaret()
        rtcHistory.ScrollBars = ScrollStateFullDrag

        My.Application.Log.DefaultFileLogWriter.BaseFileName = System.IO.Path.Combine(Application.StartupPath + "\" + "History", "" + My.Settings.LogFName)
    End Sub

