    Private Sub butp1addhotel_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles butp1addhotel.Click
        NetworkConnection()
        If My.Settings.TexttoSpeech = "Allow" Then
            SAPI.speak("Adding Hotel to " & p1n)
        End If

        My.Application.Log.WriteEntry("Adding Hotel to " & p1n & My.Computer.Clock.GmtTime.ToString, TraceEventType.Information, 4)
        My.Application.Log.WriteEntry("Money With " + p1n & " " & Currency + " " & p1c, TraceEventType.Information, 4)
        My.Application.Log.WriteEntry("Property With " + p1n & " " & p1property, TraceEventType.Information, 4)
        My.Application.Log.WriteEntry("House(s) With " + p1n & " " & p1house, TraceEventType.Information, 4)
        My.Application.Log.WriteEntry("Hotel(s) With " + p1n & " " & p1hotel, TraceEventType.Information, 4)
        My.Application.Log.WriteEntry("Money in Bank " & Currency + " " & bc, TraceEventType.Information, 4)
        My.Application.Log.WriteEntry("House(s) with Bank " & houses, TraceEventType.Information, 4)
        My.Application.Log.WriteEntry("Hotel(s) with Bank " & hotels, TraceEventType.Information, 4)
        pnlP1.Visible = True
        If (np >= 1) Then
            p1hotelcost = InputBox("Please enter the cost of one hotel which you want to purchase", "Hotel construction ( " & p1n & " )")
            My.Application.Log.WriteEntry("1 hotel of " & Currency + " " & p1hotelcost & " Is to be added to " + p1n, TraceEventType.Information, 4)
            If p1hotelcost <= p1c And p1hotelcost > 0 Then
                If hotels > 0 Then
                    p1c = Val(p1c - p1hotelcost)
                    bc = Val(bc + p1hotelcost)
                    p1hotel = Val(p1hotel + 1)
                    hotels = Val(hotels - 1)

                    lblp1cost.Text = Val(p1c)
                    lblBankCost.Text = Val(bc)
                    lblp1hotel.Text = Val(p1hotel)
                    lblHotelLeft.Text = Val(hotels)

                    If Date.Now.Hour > starttime And Date.Now.Hour < endtime Then
                    Else
                        If p1hotel <= CriticalProperty Then
                            If p1c <= CriticalAmount Then
                                lblp1cost.ForeColor = Color.Red
                                lblp1Currency.ForeColor = Color.Red
                            Else
                                lblp1cost.ForeColor = Color.LightGreen
                                lblp1Currency.ForeColor = Color.LightGreen
                            End If
                        Else
                            lblp1hotel.ForeColor = Color.LightGreen
                        End If
                    End If
                    If My.Settings.Sound_SFX = True Then
                        My.Computer.Audio.Play(My.Resources._Property, AudioPlayMode.Background)
                    End If
                    My.Application.Log.WriteEntry("Hotel added", TraceEventType.Information, 4)
                    MsgBox("Hotel is added to " + p1n)
