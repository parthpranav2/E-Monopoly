    Private Sub PrimaryFolder()
        If frmGameModeSelector.lblGMSRGHistory.Text = 0 Then
            If Directory.Exists(folder) Then
                File.SetAttributes(folder, FileAttributes.Hidden)

            Else
                Directory.CreateDirectory(folder)
                File.SetAttributes(folder, FileAttributes.Hidden)
            End If
        ElseIf frmGameModeSelector.lblGMSRGHistory.Text = 2 Then
            If Directory.Exists(folder) Then
                File.SetAttributes(folder, FileAttributes.Hidden)

            Else
                MsgBox("Cannot load the desired date resume, Error: history folder does not exist")
            End If
        End If

    End Sub

    'Secondary File name determinor
    Dim Memory As Integer

    Private Sub SecondaryFolder()
        If frmGameModeSelector.lblGMSRGHistory.Text = 0 Then
            folder1 = Application.StartupPath + "\" + "History\" + DateTime.Now.ToString("dd-MM-yyyy")
            Dim FolderName As String

            If Directory.Exists(folder1) Then
                Memory = 0

                Do While Directory.Exists(folder1 + "_" & Memory)
                    Memory = Memory + 1

                Loop

                folder1 = Application.StartupPath + "\" + "History\" + DateTime.Now.ToString("dd-MM-yyyy") + "_" & Memory

                FolderName = DateTime.Now.ToString("dd-MM-yyyy") + "_" & Memory

                My.Settings.LogFolderName = FolderName

                Directory.CreateDirectory(folder1)

            Else
                folder1 = Application.StartupPath + "\" + "History\" + DateTime.Now.ToString("dd-MM-yyyy")

                Memory = 0

                My.Settings.LogFolderName = DateTime.Now.ToString("dd-MM-yyyy")

                Directory.CreateDirectory(folder1)

            End If
        ElseIf frmGameModeSelector.lblGMSRGHistory.Text = 2 Then
            folder1 = Application.StartupPath + "\" + "History\" + frmPlayerHistory.ListBoxD.SelectedItem.ToString
            My.Settings.LogFolderName = frmPlayerHistory.ListBoxD.SelectedItem.ToString
        End If
        My.Settings.Save()
    End Sub

    Private Sub DirectoryDistroyer()
        If Directory.Exists(folder1) Then
            My.Application.Log.DefaultFileLogWriter.Close()
            File.Delete(My.Settings.HistoryFileName)

            If Memory = 0 Then
            Else
                Memory = Memory - 1
            End If

            Directory.Delete(folder1)
        Else

        End If
        My.Settings.Save()
    End Sub

