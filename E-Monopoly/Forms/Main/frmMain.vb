    Private Sub Button2_Click_1(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles Button2.Click
        If My.Computer.Network.IsAvailable Then
            lblIConnection.Text = Online
            If Date.Now.Hour > starttime And Date.Now.Hour < endtime Then
            Else
                lblIConnection.ForeColor = Color.LightGreen
            End If
            If My.Settings.MusicSite = "Amazon Music" Then
                Process.Start("https://music.amazon.in/home?ref=dm_ws_lnd_pm_listn_pm_f82420aa-88c3-44d8-940f-fd4a1f8b5d04")
            End If
            If My.Settings.MusicSite = "Google Play Music" Then
                Process.Start("https://play.google.com/music/listen?gclid=CjwKCAjw1cX0BRBmEiwAy9tKHhBe_xSZpFZhXTUjeKoOSonr5TxCpF8Eg89oXHxzdvTnGjBZdNWpQRoCaZMQAvD_BwE&gclsrc=aw.ds&u=0#/home")
            End If
            If My.Settings.MusicSite = "Ganna" Then
                Process.Start("https://gaana.com/browse")
            End If
            If My.Settings.MusicSite = "Hungama" Then
                Process.Start("https://www.hungama.com/music/")
            End If
            If My.Settings.MusicSite = "Jio Savan" Then
                Process.Start("https://www.jiosaavn.com/")
            End If
            If My.Settings.MusicSite = "Wynk" Then
                Process.Start("https://wynk.in/music")
            End If
        Else
            lblIConnection.Text = Offline
            If Date.Now.Hour > starttime And Date.Now.Hour < endtime Then
            Else
                lblIConnection.ForeColor = Color.Red
            End If
            MsgBox("Sorry the network isn't available at this movement, we are re-directing you to the Desktop to select your song from computer . If you want to choose the song from Internet , please try again later ! ", vbOKOnly + vbExclamation)

            Me.WindowState = FormWindowState.Minimized

        End If

        SecondaryScreen()

    End Sub

