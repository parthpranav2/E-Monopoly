    Private Sub EndLogging()

        My.Application.Log.WriteEntry("Money in Bank " & Currency + " " + lblBankCost.Text, TraceEventType.Information, 0)
        My.Application.Log.WriteEntry("Property(s) remaining in Bank " + lblPropertiesLeft.Text, TraceEventType.Information, 0)
        My.Application.Log.WriteEntry("House(s) remaining in Bank " + lblHousesLeft.Text, TraceEventType.Information, 0)
        My.Application.Log.WriteEntry("Hotel(s) remaining in Bank " + lblhotel.Text, TraceEventType.Information, 0)
        My.Application.Log.WriteEntry("E-Monoppoly ended at at " & My.Computer.Clock.GmtTime.ToString)
        My.Application.Log.WriteEntry("Game Dice Total = " & gamedicetotal, TraceEventType.Information, 0)

        My.Application.Log.WriteEntry("--------------------------------------")
        My.Application.Log.WriteEntry("-----------------END------------------")
        My.Application.Log.WriteEntry("--------------------------------------")
        My.Application.Log.DefaultFileLogWriter.Close()
    End Sub

