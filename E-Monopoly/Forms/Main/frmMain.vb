    Private Sub butp1addproperty_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles butp1addproperty.Click
        NetworkConnection()
        If My.Settings.TexttoSpeech = "Allow" Then
            SAPI.speak("Adding Property to " & p1n)
        End If

        My.Application.Log.WriteEntry("Adding Property to " & p1n & My.Computer.Clock.GmtTime.ToString, TraceEventType.Information, 2)
        My.Application.Log.WriteEntry("Money With " + p1n & " " & Currency + " " & p1c, TraceEventType.Information, 2)
        My.Application.Log.WriteEntry("Property With " + p1n & " " & p1property, TraceEventType.Information, 2)
        My.Application.Log.WriteEntry("Money in Bank " & Currency + " " & bc, TraceEventType.Information, 2)
        My.Application.Log.WriteEntry("Property(s) with Bank " & properties, TraceEventType.Information, 2)
        If (np >= 1) Then
            pnlP1.Visible = True
            p1propertycost = InputBox("Please enter the cost of property which you want to purchase", "Property deal ( " & p1n & " )")
            My.Application.Log.WriteEntry("1 property of " & Currency + " " & p1propertycost & " Is to be added to " + p1n, TraceEventType.Information, 2)
            If p1propertycost <= p1c And p1propertycost > 0 Then
                If properties > 0 Then
                    'Calculations
                    p1c = Val(p1c - p1propertycost)
                    bc = Val(bc + p1propertycost)
                    p1property = Val(p1property + 1)
                    properties = Val(properties - 1)


                    lblp1cost.Text = Val(p1c)
                    lblBankCost.Text = Val(bc)
                    lblp1property.Text = Val(p1property)
                    lblPropertiesLeft.Text = Val(properties)

                    If Date.Now.Hour > starttime And Date.Now.Hour < endtime Then
                    Else
                        If p1property <= CriticalProperty Then
                            If p1c <= CriticalAmount Then
                                lblp1cost.ForeColor = Color.Red
                                lblp1Currency.ForeColor = Color.Red
                            Else
                                lblp1cost.ForeColor = Color.LightGreen
                                lblp1Currency.ForeColor = Color.LightGreen
                            End If
                        Else
                            lblp1property.ForeColor = Color.LightGreen
                        End If
                    End If
                    pnlP1.Visible = False

                    If My.Settings.DeluxeAssurance = True Then
                        frmPropertySelectorAddingP1.Show()
