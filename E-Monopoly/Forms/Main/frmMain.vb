    Private Sub butaddp1_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles butaddp1.Click
        NetworkConnection()

        If My.Settings.TexttoSpeech = "Allow" Then
            SAPI.speak("Adding Money to " + p1n)
        End If
        My.Application.Log.WriteEntry("Adding Money to " & p1n & My.Computer.Clock.GmtTime.ToString, TraceEventType.Information, 1)
        My.Application.Log.WriteEntry("Money With " + p1n & " " & Currency + " " & p1c, TraceEventType.Information, 1)
        My.Application.Log.WriteEntry("Money in Bank " & Currency + " " & bc, TraceEventType.Information, 1)
        If (np >= 1) Then
            pnlP1.Visible = True
            p1c1 = InputBox("Please einter money which is to be added in the " & p1n & "'s Account", "" & p1n)
            My.Application.Log.WriteEntry("" & Currency + " " & p1c1 & " Is to be added to " + p1n, TraceEventType.Information, 1)
            If (bc >= p1c1) Then
                p1c = Val(p1c + p1c1)
                bc = Val(bc - p1c1)
                lblBankCost.Text = Val(bc)
                lblp1cost.Text = Val(p1c)
                If Date.Now.Hour > starttime And Date.Now.Hour < endtime Then
                Else
                    If p1c <= CriticalAmount Then
                        lblp1cost.ForeColor = Color.Red
                        lblp1Currency.ForeColor = Color.Red
                    Else
                        lblp1cost.ForeColor = Color.LightGreen
                        lblp1Currency.ForeColor = Color.LightGreen
                    End If
                End If
                If My.Settings.Sound_SFX = True Then
                    My.Computer.Audio.Play(My.Resources.butAddition, AudioPlayMode.Background)
                End If
                MsgBox("Transaction complete", vbOKOnly)
                pnlP1.Visible = False
            Else
                If My.Settings.Sound_SFX = True Then
                    My.Computer.Audio.Play(My.Resources.Alarm1, AudioPlayMode.Background)
                End If
                My.Application.Log.WriteEntry("There is insufficient balance in monopoly bank ", TraceEventType.Information, 1)
                MsgBox("There is insufficient balance in Monopoly Bank", vbOKOnly + vbInformation)
                pnlP1.Visible = False
            End If
        Else
            MsgBox("You cant add money to the account of the player one who is not playing", vbOKOnly + vbCritical)
            pnlP1.Visible = False
        End If
        If Date.Now.Hour > starttime And Date.Now.Hour < endtime Then

        Else
            If p1c <= CriticalAmount Then
                lblp1cost.ForeColor = Color.Red
                lblp1Currency.ForeColor = Color.Red
            Else
                lblp1cost.ForeColor = Color.LightGreen
                lblp1Currency.ForeColor = Color.LightGreen
            End If
        End If

        If (bc >= PassG) Then
            butround.Enabled = True
        Else
            butround.Enabled = False
        End If
        If (bc >= Loan) Then
            butloan.Enabled = True
        Else
            butloan.Enabled = False
        End If
        My.Application.Log.WriteEntry("Money With " + p1n & " " & Currency + " " & p1c, TraceEventType.Information, 1)
        My.Application.Log.WriteEntry("Money in Bank " & Currency + " " & bc, TraceEventType.Information, 1)
        My.Application.Log.WriteEntry("--------------------------------------")

        SecondaryScreen()

